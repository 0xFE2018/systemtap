function rdtsc () %{
   int res[2];
   __asm__ __volatile__  ("rdtsc\n" : "=a" (res[0]), "=d" (res[1]) );
   THIS->__retvalue = *(long*)res;
%}

global process_hits
%( @2 == "NO_STAP_SDT" %?
probe process(@1).function("*").label("BENCH_PERMUTE")
{   process_hits <<< (rdtsc() - $cycles_start) }
%:
probe process(@1).mark("permute")
{   process_hits <<< (rdtsc() - $arg1) }
%)

