#! stap
global teststr
probe begin { println("systemtap starting probe") }

probe ioblock.request, ioblock.end {
   teststr = sprintf("ioblock: %s\t%d\t%s\t%d\n", devname, sector,
                      bio_rw_str(rw), bio_rw_num(rw))
}
probe end { 
   println("systemtap ending probe")
   printf("%s", teststr)
}
