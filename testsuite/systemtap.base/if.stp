/*
 * if.stp
 *
 * Check the systemtap if statement works
 */

probe begin { log("systemtap starting probe") }

probe end
{
	log("systemtap ending probe")
	if (1) {
		log("systemtap test success");
	} else {
		log("systemtap test failure");
	}
	if (0) {
		log("systemtap test failure");
	} else {
		log("systemtap test success");
	}
}
