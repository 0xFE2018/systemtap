/*
 * add.stp
 *
 * Check the systemtap "addition" works
 */

global x3
global x1
global x2

probe begin
{
	log("systemtap starting probe")
	x1 = 42; x2 = 53;
}

probe end
{
	log("systemtap ending probe")
	x3 = x1 + x2;
	if (x3 != 95 ) {
		log("systemtap test failure");
	} else {
		log("systemtap test success");
	}
}
